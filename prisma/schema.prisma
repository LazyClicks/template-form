// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Rdv {
  id             Int      @id @default(autoincrement())
  firstName      String
  lastName       String
  email          String
  phone          String
  phone2         String?
  dateOfBirth    String
  motif          String
  alreadyPatient String?  @default("false")
  doctor         String?
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
